{
    "name": "AWS X-ray",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "AWS X-ray on steroids",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "scripts/jquery-3.3.1.min.js",
            "scripts/background.js"
        ]
    },
    "commands": {
        "colorize-aws-xray": {
            "suggested_key": {
              "default": "Ctrl+Shift+X",
              "mac": "Command+Shift+X"
            },
            "description": "Colorize AWS X-ray"
          }
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "scripts/jquery-3.3.1.min.js",
                "scripts/content.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ]
}